{
  "name": "codex.docs",
  "license": "Apache-2.0",
  "version": "2.3.0",
  "bin": {
    "codex.docs": "dist/backend/app.js"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%"
  ],
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "concurrently \"yarn start:backend\" \"yarn build:frontend\"",
    "dev": "concurrently \"yarn start:backend:dev\" \"yarn start:frontend:dev\"",
    "start:backend": "cross-env NODE_ENV=development ts-node src/backend/app.ts -c docs-config.yaml -c docs-config.local.yaml",
    "start:backend:dev": "cross-env NODE_ENV=development npx nodemon --config nodemon.json src/backend/app.ts -c docs-config.yaml -c docs-config.local.yaml",
    "start:frontend:dev": "webpack --mode=development --watch",
    "build:static": "ts-node src/backend/app.ts build-static -c docs-config.yaml -c docs-config.local.yaml",
    "build:all": "yarn build:frontend && yarn build:backend",
    "build:backend": "tsc --project tsconfig.backend.json && copyfiles -u 3 ./src/**/*.twig ./dist/backend/views && copyfiles -u 1 ./src/**/*.svg ./dist/",
    "build:frontend": "webpack --mode=production",
    "test:js": "cross-env NODE_ENV=testing mocha --recursive ./dist/test --exit",
    "test": "cross-env NODE_ENV=testing ts-mocha -n loader=ts-node/esm ./src/test/*.ts ./src/test/**/*.ts --exit ",
    "lint": "eslint --fix --ext .ts ./src/backend",
    "editor-upgrade": "yarn add -D @editorjs/{editorjs,header,code,delimiter,list,link,image,table,inline-code,marker,warning,checklist,raw,quote,embed,underline}@latest"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.181.0",
    "@codex-team/config-loader": "1.0.0",
    "@codexteam/shortcuts": "^1.2.0",
